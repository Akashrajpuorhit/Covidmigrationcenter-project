<script>
        $(document).ready(function() {
            
                $('.dl_btn').click(function() {
                    if (window.localStorage) {
                        const resolution = $('tr.selected td').eq(0).text();
                        window.localStorage.setItem('resolution', resolution);
                    }
                });
            
            if ('URLSearchParams' in window) {
                let searchParams = new URLSearchParams(window.location.search)
                if (searchParams.has('download') && window.localStorage) {
                    const resolution = window.localStorage.getItem('resolution');
                    if (resolution === null) return
                    $(`tr:contains(${resolution})`).click();
                    $('.dl_btn')[0].click();
                    $('#say_thanks_overlay').fadeIn();
                }
            }
        });
    </script>
